# -*- mode: yaml -*-

manifest:
  version: 1.0

automations:
  assign_code_experts:
    if: 
      - {{ source.diff.files | map(attr='new_file') }}
      - {{ source.diff.files | map(attr='new_content') }}
    run:
      - action: add-label@v1
        args:
          label: 'deprecated-component'
          color: '#FF0000'
